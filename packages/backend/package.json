{
  "name": "@complianceos/backend",
  "version": "1.0.0",
  "description": "Enterprise backend API for ComplianceOS with Express, tRPC, and advanced security",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "db:migrate": "cd ../database && npm run migrate",
    "db:seed": "cd ../database && npm run seed",
    "docker:build": "docker build -t complianceos-backend .",
    "docker:run": "docker run -p 8000:8000 complianceos-backend"
  },
  "dependencies": {
    "@complianceos/types": "workspace:*",
    "@complianceos/auth": "workspace:*",
    "@trpc/server": "^10.45.0",
    "@trpc/client": "^10.45.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.4",
    "superjson": "^2.2.1",
    "ioredis": "^5.3.2",
    "bull": "^4.12.0",
    "nodemailer": "^6.9.7",
    "pdf-lib": "^1.17.1",
    "sharp": "^0.32.6",
    "archiver": "^6.0.1",
    "node-cron": "^3.0.3",
    "swagger-ui-express": "^5.0.0",
    "openapi-types": "^12.1.3",
    "swagger-jsdoc": "^6.2.8",
    "dotenv": "^16.3.1",
    "express-openapi-validator": "^5.1.4",
    "express-prometheus-middleware": "^1.2.0",
    "prom-client": "^15.1.0",
    "joi": "^17.11.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "ws": "^8.14.2",
    "socket.io": "^4.7.4",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "axios": "^1.6.2",
    "form-data": "^4.0.0",
    "mime-types": "^2.1.35",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.17",
    "pg": "^8.11.3",
    "redis": "^4.6.11"
  },
  "devDependencies": {
    "@complianceos/eslint-config": "workspace:*",
    "@complianceos/typescript-config": "workspace:*",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/multer": "^1.4.11",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "@types/ws": "^8.5.10",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/nodemailer": "^6.4.14",
    "@types/archiver": "^6.0.2",
    "@types/node-cron": "^3.0.11",
    "@types/express-session": "^1.17.10",
    "@types/pg": "^8.10.9",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}